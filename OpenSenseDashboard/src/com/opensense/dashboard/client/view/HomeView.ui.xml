<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:d="urn:import:com.opensense.dashboard.client.utils"
	xmlns:ma="urn:import:gwt.material.design.addins.client"
	xmlns:h="urn:import:gwt.material.design.client.ui.html">

	<ui:with type="com.opensense.dashboard.client.utils.Languages"
		field="lang" />
	<ui:with
		type="com.opensense.dashboard.client.gui.GUIImageBundle" field="gui" />

	<ui:style>
	
	</ui:style>

	<bh:Div ui:field="dashboardinit">
		<m:MaterialButton ui:field="welcomeUser"
			width="500px" height="200px" text="Wilkommen: {lang.username}"
			textColor="WHITE" addStyleNames="d-btn" />

		<m:MaterialButton ui:field="visuInfo" width="500px"
			height="200px" text="{lang.accuracy}" textColor="WHITE"
			addStyleNames="d-btn" />
		<m:MaterialButton ui:field="searchInfo" width="100%"
			height="200px" text="{lang.search}" textColor="WHITE"
			addStyleNames="d-btn" />
		<m:MaterialButton ui:field="mapInfo" width="600px"
			height="400px" text="{lang.map}" textColor="WHITE"
			addStyleNames="d-btn" />


	</bh:Div>

</ui:UiBinder>