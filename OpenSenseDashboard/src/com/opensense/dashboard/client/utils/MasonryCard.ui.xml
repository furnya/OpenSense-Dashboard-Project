<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:bh="urn:import:org.gwtbootstrap3.client.ui.html"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:d="urn:import:com.opensense.dashboard.client.utils"
	xmlns:ma="urn:import:gwt.material.design.addins.client">

	<ui:with type="com.opensense.dashboard.client.utils.Languages"
		field="lang" />
	<ui:with
		type="com.opensense.dashboard.client.gui.GUIImageBundle" field="gui" />

	<ui:style>
	.add-sensor-modal{
	}
	.max> div > ul {
    max-height: 125px!important;
	}
	.mapContainer{
		width: 60%;
		height: 100%;
	}
	.locContainer{
		width: 100%;
		height: 70%;
	}
	.locBoxContainer{
		width: 40%;
		padding: 10px;
	}
	.locBoxContainer > *{
		width: 100%;
	}
	.modalHeader{
		font-size: 200%;
		text-transform: uppercase;
	}
	</ui:style>
	<ma:masonry.MaterialMasonry ui:field="masonry">
		<m:MaterialColumn grid="l4 s12 m6" />
		<m:MaterialColumn grid="l4 s12 m6" />
		<m:MaterialColumn grid="l4 s12 m6" />
	</ma:masonry.MaterialMasonry>
</ui:UiBinder>